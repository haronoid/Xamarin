		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		x:Class="HelloWorld.MessageCenterPage">
	<ContentPage.Padding>
		<OnPlatform x:TypeArguments="Thickness" iOS="0,20,0,0" />
	</ContentPage.Padding>

	<StackLayout>
		<Label x:Name="lblStatus" Text="" />
		<Button Text="New Page" Clicked="Handle_Clicked" />
		<Button Text="Close" Clicked="Close_Clicked"/>
	</StackLayout>
	
	
</ContentPage>
